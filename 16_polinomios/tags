!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.9~svn20110310	//
ANSI_COLOR_BLUE	ansi_colors.h	7;"	d
ANSI_COLOR_CYAN	ansi_colors.h	9;"	d
ANSI_COLOR_GREEN	ansi_colors.h	5;"	d
ANSI_COLOR_MAGENTA	ansi_colors.h	8;"	d
ANSI_COLOR_RED	ansi_colors.h	4;"	d
ANSI_COLOR_RESET	ansi_colors.h	10;"	d
ANSI_COLOR_YELLOW	ansi_colors.h	6;"	d
CC	Makefile	/^CC=g++$/;"	m
CFLAGS	Makefile	/^CFLAGS=-g$/;"	m
C_FN	interfaz.cpp	10;"	d	file:
DELTA	polinomios.h	4;"	d
DEPS	Makefile	/^DEPS=$(wildcard *.h)$/;"	m
ENTORNO	interfaz.h	5;"	d
FN_MAX	interfaz.cpp	11;"	d	file:
INC	interfaz.cpp	6;"	d	file:
MED	interfaz.cpp	8;"	d	file:
OBJ	Makefile	/^OBJ=$(SRC:.cpp=.o)$/;"	m
OUT	Makefile	/^OUT=pol$/;"	m
PASOS	interfaz.h	4;"	d
Polinomio	polinomios.h	/^struct Polinomio {$/;"	s
SRC	Makefile	/^SRC=$(wildcard *.cpp)$/;"	m
__ANSI_COLORS_	ansi_colors.h	2;"	d
__INTERFAZ_H__	interfaz.h	2;"	d
__POLINOMIOS_H__	polinomios.h	2;"	d
coef	polinomios.h	/^    double *coef;$/;"	m	struct:Polinomio
derivada	polinomios.cpp	/^double derivada(struct Polinomio pol, double x){ return (f(pol, x+DELTA) -f(pol,x)) \/ DELTA; }$/;"	f
f	polinomios.cpp	/^double f(struct Polinomio polinomio, double x) {$/;"	f
grado	polinomios.h	/^    int     grado;$/;"	m	struct:Polinomio
main	main.cpp	/^int main(int argc, char *argv[]){$/;"	f
pedir_polinomio	interfaz.cpp	/^void pedir_polinomio(struct Polinomio *p){$/;"	f
pedir_punto	interfaz.cpp	/^void pedir_punto(double *x) {$/;"	f
pinta_entorno	interfaz.cpp	/^void pinta_entorno(struct Polinomio pol, double x){$/;"	f
titulo	interfaz.cpp	/^void titulo(){$/;"	f
